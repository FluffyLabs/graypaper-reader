{
  "name": "graypaper-reader",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": ["tools/*", "shared/*"],
  "scripts": {
    "qa": "biome ci",
    "qa-fix": "npm run format; npm run lint",
    "format": "biome format --write",
    "lint": "biome lint --write; biome check --write",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "cp-gp": "mkdir -p ./public && cp -r ./graypaper-archive/dist/* ./public/",
    "prebuild": "git submodule update --init && npm run cp-gp",
    "predev": "npm run cp-gp"
  },
  "dependencies": {
    "@fluffylabs/migrate-selection": "^1.0.0",
    "@fluffylabs/synctex-store": "^1.0.0",
    "dompurify": "^3.2.0",
    "katex": "^0.16.11",
    "pdfjs-dist": "^4.5.136",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-tooltip": "^5.27.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@fluffylabs/types": "^1.0.0",
    "@types/katex": "^0.16.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-prefetch-chunk": "^0.1.2",
    "vitest": "^2.1.8"
  }
}
